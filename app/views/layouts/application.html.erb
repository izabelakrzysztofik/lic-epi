</head>
<body>

  <% if notice %>
  <script type="text/javascript">
 Materialize.toast('<%= notice %>', 4000)
</script>
  	<% end %>
  	<% if alert %>
    <script type="text/javascript">
    Materialize.toast('<%= alert %>', 4000)
  </script>
  	<% end %>
